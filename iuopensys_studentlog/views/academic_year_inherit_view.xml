<?xml version="1.0" ?>
<openerp>
	<data>
		<record id="academic_year_inherit_form_view" model="ir.ui.view">
			<field name="name">academic.year.form.inherit</field>
			<field name="model">academic.year</field>
			<field name="inherit_id" ref="iuopensys_core.academic_year_form_view"/>
			<field name="arch" type="xml">
				<xpath expr="//group[1]" position="after">
					<notebook>
						<page string="Student List">
							<group>
								<field name="student_ids" nolabel="1"/>
							</group>
						</page>
					</notebook>
				</xpath>
			</field>
		</record>
	</data>
</openerp>