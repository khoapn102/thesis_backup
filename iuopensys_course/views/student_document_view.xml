<?xml version="1.0" ?>
<openerp>
	<data>
		<record model="ir.actions.act_window" id="student_document_main_action">
			<field name="name">Student Documents</field>
			<field name="res_model">student.document</field>
			<field name="view_mode">tree,form</field>
		</record>
		
		<record id="student_document_tree_view" model="ir.ui.view">
			<field name="name">Student Documents</field>
			<field name="model">student.document</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree>						
					<field name="student_id"/>
					<field name="iu_doc_id"/>
					<field name="is_submit"/>
					<field name="is_stored"/>
				</tree>
			</field>
		</record>
		
		<record id="student_document_form_view" model="ir.ui.view">
			<field name="name">Document</field>
			<field name="model">student.document</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form>					
					<sheet>
						<group>
							<group>
								<field name="student_id"/>
								<field name="is_submit"/>
							</group>
							<group>
								<field name="iu_doc_id"/>
								<field name="is_stored"/>
							</group>
						</group>
						<group>
							<field name="attachment_ids" nolabel="1" context="{'default_student_document_id':active_id}">
								<tree editable="bottom">
									<field name="name"/>
									<field name="type" readonly="1"/>									
									<field name="create_date" string="Date Submitted"/>
									<field name="db_datas"/>
									<field name="student_document_id" invisible="1"/>
								</tree>
							</field>
						</group>					
					</sheet>
				</form>
			</field>
		</record>
		
		<!-- Search View -->
		<record id="" model="ir.ui.view">
			<field name="name"></field>
			<field name="model"></field>
			<field name="type">search</field>
			<field name="arch" type="xml">
				<search>
					<!--Search By -->
					
					<!--Group By -->
				</search>
			</field>
		</record>
	</data>
</openerp>